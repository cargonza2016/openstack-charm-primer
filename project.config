[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/cargonza2016
[access "refs/for/refs/*"]
	read = group user/cargonza2016
	push = group user/cargonza2016
	label-Code-Review = -2..+2 group user/cargonza2016
	label-Verified = -1..+1 group user/cargonza2016
	rebase = group user/cargonza2016
	removeReviewer = group user/cargonza2016
	submit = group user/cargonza2016
[access "refs/heads/*"]
	read = group user/cargonza2016
	create = group user/cargonza2016
	push = +force group user/cargonza2016
	pushMerge = group user/cargonza2016
[access "refs/tags/*"]
	push = +force group user/cargonza2016
	pushTag = group user/cargonza2016
	pushSignedTag = group user/cargonza2016
